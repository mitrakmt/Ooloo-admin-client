{
  "extends": ["airbnb"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 2,
    "maximumLineLength": 120,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "tabWidth": 2,
        "useTabs": false,
        "semi": false
      }
    ]
  }
}
